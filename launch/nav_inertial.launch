<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="MOVE_INERTIAL" output="screen">
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/move_base_params.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher_sitl/params/dwa_local_planner_params.yaml" command="load" />
    <remap from="/odom" to ="/odom_inertial" />
  </node>

  <node pkg="clear_costmap" type="clear_costmap.py" respawn="false" name="clear_costmap" output="screen" />

</launch> 
